<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="IsMaterMapper">

<insert id="insert">
INSERT INTO IS_MATER VALUES(
	ismater_seq.NEXTVAL,
	#{korName},
	#{juminNo},
	#{engName},
	#{sex},
	#{lastSchool},
	#{major},
	#{graduDate},
	#{comLag},
	#{licence1},
	#{licence2},
	#{licence3},
	#{techLev},
	#{address},
	#{workDate},
	#{state},
	#{image},
	#{workFlag},
	#{phone},
	#{email}
)

</insert>


<select id="selectAll"  resultType="IsMater" parameterType="java.util.Map">

<![CDATA[
select * from (SELECT * FROM ( SELECT rownum rnum,KOR_NAME,JUMIN_NO,WORK_DATE,WORK_FLAG,TECH_LEV FROM IS_MATER)
where rnum <= #{endPage} ) where rnum >= #{startPage}
]]>

</select>

<select id="selectAllList"  resultType="IsMater">
select * from IS_MATER
</select>


<!-- select rownum, a.*
from is_mater a
where rownum between #{startPage} and #{endPage} -->


</mapper>